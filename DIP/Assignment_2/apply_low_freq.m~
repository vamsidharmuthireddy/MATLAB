function img_low = apply_low_freq( img,freq )
%APPLY_LOW_FREQ Summary of this function goes here
%   Detailed explanation goes here
[r,c,ch] = size(img);

x0=floor(c/2);
y0=floor(r/2);

a = fft2(img);

for z=1:ch
    for x=1:c
        for y=1:r
            dist = (x0-x)^2+(y0-y)
            
            
        end
    end
end


img_low = ifft2(a);





end

